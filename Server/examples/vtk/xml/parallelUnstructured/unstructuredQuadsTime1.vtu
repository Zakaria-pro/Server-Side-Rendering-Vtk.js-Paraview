<?xml version="1.0"?>
<!-- Lets make 4 2d quads from 9 points -->
<!-- When we need timesteps, this will be time 1 -->
<!-- Create some cell data -->
<!-- Create some points -->
<!-- Create some cells from the points-->

<VTKFile type="UnstructuredGrid" version="0.1" byte_order="LittleEndian" compressor="vtkZLibDataCompressor">
  <UnstructuredGrid>
    <Piece NumberOfPoints="9" NumberOfCells="4">
      <CellData>
        <DataArray type="Float32" Name="temperature" format="ascii">
          31 33 35 37 
        </DataArray>
        <DataArray type="Float32" Name="pressure" format="ascii">
          2 4 6 8 
        </DataArray>
      </CellData>
      <Points>
        <DataArray type="Float32" NumberOfComponents="3" format="ascii">
          0 0 4 -1 -1 4
          1 -1 4 1 1 4
          -1 1 4 0 -1 4
          1 0 4 0 1 4
          -1 0 4 
        </DataArray>
      </Points>
      <Cells>
        <DataArray type="Int32" Name="connectivity" format="ascii">
          1 5 0 8 
          5 2 6 0 
          0 6 3 7
          0 7 4 8
        </DataArray>
        <DataArray type="Int32" Name="offsets" format="ascii">
          4 8 12 16
        </DataArray>
        <DataArray type="UInt8" Name="types" format="ascii">
         9 9 9 9
        </DataArray>
      </Cells>
    </Piece>
  </UnstructuredGrid>
</VTKFile>
